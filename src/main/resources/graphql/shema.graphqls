type Player {
    id: ID!
    name: String!
    surname: String!
    position: Positions!
}

enum Positions{
    PG,
    SG,
    SF,
    PF,
    C
}

input PlayerInput{
    name: String!
    surname: String!
    position: String!
}

type Query {
    players: [Player!]!
}

type Mutation {
    addPlayer(playerInput : PlayerInput) : Player!
    deletePlayer(id: ID) : ID
}